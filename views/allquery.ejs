<!DOCTYPE html>
<html lang="en">
<head>
    <meta charset="UTF-8">
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <title>Document</title>
    <%- include('../partials/headlink.ejs') %>
</head>
<body>
    <%- include('../partials/adminuserprofileheader.ejs') %>
   <div class="deshboard-mid">
    <div class="row">
        <%- include('../partials/deshboard-link.ejs') %>
        <div class="col-9">
           <div class="user-heading">
            <div class="row">
                <div class="col-3">
                   <p>Name</p> 
                </div>
                <div class="col-3">
                    <p>Mobile</p> 
                </div>
                <div class="col-6">
                    <p>Message</p> 
                </div>
            </div>
           </div>
           <div class="user-items">
            <% allquery.forEach(item => { %>
            <div class="user-item">
                <div class="row">
                    <div class="col-3">
                       <p><%= item.name %></p> 
                    </div>
                    <div class="col-3">
                        <p><%= item.mobile %></p> 
                    </div>
                    <div class="col-5">
                        <p><%= item.message %></p> 
                    </div>
                    <div class="col-1">
                        <a href="/admin/deletequery/<%= item.id %>">
                            <i class="fa-solid fa-trash-can"></i>
                        </a>
                    </div>
                </div>
            </div>
            <% }) %>
           </div>
        </div>
    </div>
   </div>
    
</body>
</html>