<!DOCTYPE html>
<html lang="en">
<head>
    <meta charset="UTF-8">
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <title>Document</title>
    <%- include('../partials/headlink.ejs') %>
</head>
<body>
    <%- include('../partials/adminuserprofileheader.ejs') %>
   <div class="deshboard-mid">
    <div class="row">
        <%- include('../partials/deshboard-link.ejs') %>
        <div class="col-9">
            <form action="/admin/alluser/search" method="get" class="mt-3 mb-2 d-flex">
                <input type="text" name="address" placeholder="Search by Address" class="form-control w-25 me-2" />
                <button type="submit" class="btn-book">Search</button>
             </form>
           <div class="user-heading">
            <div class="row">
                <div class="col-3">
                   <p>Name</p> 
                </div>
                <div class="col-2">
                    <p>Mobile</p> 
                </div>
                <div class="col-3">
                    <p>Address</p> 
                </div>
                <div class="col-2">
                    <p>working</p> 
                </div>
                <div class="col-2">
                    <p>degree</p> 
                </div>
                
                
            </div>
           </div>
           <div class="user-items">
            <% alluser.forEach(user => { %>
            <div class="user-item">
                <div class="row">
                    <div class="col-3">
                       <p><%= user.name %></p> 
                    </div>
                    <div class="col-2">
                        <p><%= user.mobile %></p> 
                    </div>
                    <div class="col-3">
                        <p><%= user.address1 %></p> 
                    </div>
                    <div class="col-2">
                        <p><%= user.working %></p> 
                    </div>
                    <div class="col-2">
                        <p><%= user.degree %></p> 
                    </div>
                </div>
            </div>
            <% }) %>
           </div>
        </div>
    </div>
   </div>
    
</body>
</html>